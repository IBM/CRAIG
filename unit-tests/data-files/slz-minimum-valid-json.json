            "public_gateway": false
          },
          {
            "acl_name": "management-acl",
            "cidr": "10.20.20.0/24",
            "name": "vpe-zone-2",
            "public_gateway": false
          }
        ],
        "zone-3": [
          {
            "acl_name": "management-acl",
            "cidr": "10.30.10.0/24",
            "name": "vsi-zone-3",
            "public_gateway": false
          },
          {
            "acl_name": "management-acl",
            "cidr": "10.30.20.0/24",
