      "ssh_keys": ["slz-ssh-key"],
      "subnet_names": ["vsi-zone-1", "vsi-zone-2", "vsi-zone-3"],
      "vpc_name": "workload",
      "vsi_per_subnet": 1
    }
  ],
  "wait_till": "IngressReady"
}
